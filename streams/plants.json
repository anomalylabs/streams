{
    "slug": "plants",
    "route": "garden/{slug}",
    "template": "plant",
    
    "fields": {
        "name": "text",
        "description": "textarea",
        "life_cycle": {
            "type": "select",
            "options": [
                "Perennial",
                "Annual",
                "Biennial"
            ]
        },
        "type": {
            "type": "select",
            "options": [
                "Fruit",
                "Vegetable"
            ]
        }
    }
}
